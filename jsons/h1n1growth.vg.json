{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
        "text": "New cases of influenza overtime (2004-2020)"
    },
    "data": {
        "url": "https://raw.githubusercontent.com/yyon0014/3179/refs/heads/main/data/influenza_weekly.csv",
        "format": {
            "type": "csv"
        }
                    
    },
    "config":{
    },
    "width": 250,
    "height": 250,
    "mark": "area",
    "background": "#fffeee",
    "encoding": {
        "x": {"field": "SDATE", "type": "temporal", "title": "Date"},
        "y": {"aggregate":"sum", "field": "SPEC_PROCESSED_NB", "type": "quantitative", "title": "New Cases"},
        "color": {"field": "Country", "type": "nominal"},
        "tooltip":[
        {"field": "Country", "title": "Location", "type": "nominal"},
        {"field": "SPEC_PROCESSED_NB", "title": "New Cases", "type": "quantitative"},
        {"field": "SDATE", "title": "Date", "type": "temporal"}
        ]
    }
    
}