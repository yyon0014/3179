{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
        "text": "Top 5 Countries Accumulated Mpox Cases Overtime"
    },
    "data": {
        "url": "https://raw.githubusercontent.com/yyon0014/3179/refs/heads/main/data/mpoxt5prog.csv",
        "format": {
            "parse": {
                "date": "%d/%m/%Y"
            }
        }
    },
    "width": 400,
    "height": 400,
    "mark": "line",
    "encoding": {
        "x": {"field": "date", "type": "ordinal"},
        "y": {"field": "total_cases", "type": "quantitative"}
    }
}