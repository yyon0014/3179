{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
        "text": "Top 5 Countries Accumulated Mpox Cases Overtime"
    },
    "data": {
        "url": "https://raw.githubusercontent.com/yyon0014/3179/refs/heads/main/data/mpoxt5prog.csv",
        "format": {
            "type": "csv"
        }
                    
    },
    "config":{
    },
    "width": 400,
    "height": 400,
    "mark": "line",
    "background": "#fffeee",
    "encoding": {
        "x": {"field": "date", "type": "temporal"},
        "y": {"field": "total_cases", "type": "quantitative"},
        "color": {"field": "location", "type": "nominal"},
        "tooltip":[
        {"field": "location", "title": "Location", "type": "nominal"},
        {"field": "total_cases", "title": "Registered Cases", "type": "quantitative"},
        {"field": "date", "title": "Date", "type": "temporal"}
        ]
    }
    
}